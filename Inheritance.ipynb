    "###### Method Chaining\n",
    "class Student:\n",
    "    def class1(self):\n",
    "        print(\"day1\")\n",
    "\n",
    "class Student2(Student):\n",
    "    def class1(self):\n",
    "        super().class1()\n",
    "        print(\"day2\")\n",
    "\n",
    "Std=Student2()\n",
    "Std.class1()"
   ]
