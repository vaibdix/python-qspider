    "        # print(dir(Dad))\n",
    "\n",
    "class Son(Dad):\n",
    "    def enjoy(self):\n",
    "        print(\"Bike\")\n",
    "        # print(dir(Son))\n",
    "\n",
    "s=Son()\n",
    "s.Property()\n",
    "s.Money()\n",
    "s.enjoy()"
